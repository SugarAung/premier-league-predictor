# Environment
venv/
__pycache__/
*.pyc

# Raw data (safe to ignore)
data/raw/

# DO NOT ignore processed essential files
# Keep feature + model input data
!data/processed/
!data/processed/*.csv

# Ignore backup or temporary CSVs
data/processed/*_backup.csv
data/processed/*_temp.csv

# Live outputs (optional to ignore)
data/live/*_backup.csv

# DON'T ignore model files (needed for predictions!)
!models/
!models/*.pkl

# Ignore large optional models
models/*_old.pkl
models/*_test.pkl

# Jupyter
.ipynb_checkpoints/

# Logs
*.log

# Streamlit uploads
.streamlit/

# IDE / OS files
.vscode/
.DS_Store

# API key
my_api_key.txt

# Ignore temporary prediction outputs (but keep final)
pl_predictions_backup.csv

# KEEP essential live data files
!data/live/
!data/live/pl_predictions.csv
!data/live/pl_results_from_api.csv
!data/live/pl_fixtures_from_api.csv
!data/live/live_predictions_history.csv
